            "location": "examples/yaml",
            "analysisMode": "full",
            "providerSpecificConfig": {
                "name": "yq",
                "lspServerPath": "/usr/bin/yq"
            }
        }]
    },
    {
        "name": "python",
        "binaryPath": "/usr/bin/generic-external-provider",
        "initConfig": [{
            "location": "examples/python",
            "analysisMode": "full",
            "providerSpecificConfig": {
                "name": "python",
                "lspServerPath": "/usr/local/bin/pylsp",
